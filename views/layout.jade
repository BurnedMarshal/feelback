doctype html
html
  // Favicons
  link(rel='icon', href='images/favicon/favicon-32x32.png', sizes='32x32')
  // Favicons
  link(rel='apple-touch-icon-precomposed', href='images/favicon/apple-touch-icon-152x152.png')
  // For iPhone
  meta(name='msapplication-TileColor', content='#00bcd4')
  meta(name='msapplication-TileImage', content='images/favicon/mstile-144x144.png')
  // For Windows Phone
  // CORE CSS
  link(href='stylesheets/materialize.css', type='text/css', rel='stylesheet', media='screen,projection')
  link(href='stylesheets/style.css', type='text/css', rel='stylesheet', media='screen,projection')
  // INCLUDED PLUGIN CSS ON THIS PAGE
  link(href='javascript/plugins/perfect-scrollbar/perfect-scrollbar.css', type='text/css', rel='stylesheet', media='screen,projection')
  link(href='javascript/plugins/jvectormap/jquery-jvectormap.css', type='text/css', rel='stylesheet', media='screen,projection')
  link(href='javascript/plugins/chartist-js/chartist.min.css', type='text/css', rel='stylesheet', media='screen,projection')
  body
    // Start Page Loading
    #loader-wrapper
      #loader
      .loader-section.section-left
      .loader-section.section-right
    // End Page Loading
    // ////////////////////////////////////////////////////////////////////////////
    // START HEADER
    header#header.page-topbar
      // start header nav
      .navbar-fixed
        nav.cyan
          .nav-wrapper
            h1.logo-wrapper
              a.brand-logo.darken-1(href='index.html')
                img(src='images/materialize-logo.png', alt='materialize logo')
              span.logo-text Materialize
            ul.right.hide-on-med-and-down
              li.search-out
                input.search-out-text(type='text')
              li
                a.waves-effect.waves-block.waves-light.show-search(href='javascript:void(0);')
                  i.mdi-action-search
              //li
                a.waves-effect.waves-block.waves-light.toggle-fullscreen(href='javascript:void(0);')
                  i.mdi-action-settings-overscan
              //li
                a.waves-effect.waves-block.waves-light(href='javascript:void(0);')
                  i.mdi-social-notifications
              // Dropdown Trigger
              //li
                a.waves-effect.waves-block.waves-light.chat-collapse(href='#', data-activates='chat-out')
                  i.mdi-communication-chat
        // end header nav
      // END HEADER
      block content
      footer.page-footer
        .container
          .row
            .col.l6.s12
              h5.white-text World Market
              p.grey-text.text-lighten-4 World map, world regions, countries and cities.
              #world-map-markers
            .col.l4.offset-l2.s12
              h5.white-text Sales by Country
              p.grey-text.text-lighten-4 A sample polar chart to show sales by country.
              #polar-chart-holder
                canvas#polar-chart-country(width='200')
        .footer-copyright
          .container
            | Copyright Â© 2015 
            a.grey-text.text-lighten-4(href='http://themeforest.net/user/geekslabs/portfolio?ref=geekslabs', target='_blank') GeeksLabs
            |  All rights reserved.
            span.right
              | Design and Developed by 
              a.grey-text.text-lighten-4(href='http://geekslabs.com/') GeeksLabs
      // END FOOTER
      //
        ================================================
        Scripts
        ================================================
      // jQuery Library
      script(type='text/javascript', src='javascripts/jquery-1.11.2.min.js')
      // materialize js
      script(type='text/javascript', src='javascripts/materialize.min.js')
      // scrollbar
      script(type='text/javascript', src='javascripts/plugins/perfect-scrollbar/perfect-scrollbar.min.js')
      // chartist
      script(type='text/javascript', src='javascripts/plugins/chartist-js/chartist.min.js')
      // chartjs
      script(type='text/javascript', src='javascripts/plugins/chartjs/chart.min.js')
      script(type='text/javascript', src='javascripts/plugins/chartjs/chart-script.js')
      // sparkline
      script(type='text/javascript', src='javascripts/plugins/sparkline/jquery.sparkline.min.js')
      script(type='text/javascript', src='javascripts/plugins/sparkline/sparkline-script.js')
      // jvectormap
      script(type='text/javascript', src='javascripts/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js')
      script(type='text/javascript', src='javascripts/plugins/jvectormap/jquery-jvectormap-world-mill-en.js')
      script(type='text/javascript', src='javascripts/plugins/jvectormap/vectormap-script.js')
      // plugins.js - Some Specific JS codes for Plugin Settings
      script(type='text/javascript', src='javascripts/plugins.js')
