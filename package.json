{
  "name": "feelback",
  "author": "Daniele Manni",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint ./**/**.js",
    "start": "forever start --uid ${npm_package_name} --append bin/www",
    "restart": "forever restart --uid ${npm_package_name} bin/www",
    "stop": "forever stop --uid ${npm_package_name} bin/www",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- ./tests/user.js ./tests/data.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "forever": "^0.15.2",
    "jade": "~1.11.0",
    "jwt-simple": "^0.5.0",
    "moment": "^2.14.1",
    "morgan": "~1.7.0",
    "node-uuid": "^1.4.7",
    "seraph": "^0.15.2",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "eslint": "^3.3.0",
    "eslint-config-google": "^0.6.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-commit": "^1.1.3"
  }
}
