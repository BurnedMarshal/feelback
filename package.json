    "debug": "~2.2.0",
    "express": "~4.13.4",
    "forever": "^0.15.2",
    "jade": "~1.11.0",
    "jwt-simple": "^0.5.0",
    "moment": "^2.14.1",
    "morgan": "~1.7.0",
    "node-uuid": "^1.4.7",
    "seraph": "^0.15.2",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "eslint": "^3.3.0",
    "eslint-config-google": "^0.6.0",
    "pre-commit": "^1.1.3",
    "supertest": "^2.0.0",
    "tape": "^4.6.0"
  }
